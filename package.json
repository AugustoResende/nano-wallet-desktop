{
  "name": "@nanocurrency/nano-desktop",
  "version": "1.0.0-beta.7",
  "productName": "Nano Desktop",
  "description": "The official Nano wallet for desktop and web",
  "license": "BSD-2-Clause",
  "author": "Nano Wallet Company <desktop@nanowalletcompany.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanocurrency/nano-desktop.git"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember serve",
    "test": "ember test",
    "lint:js": "eslint .",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "cz": "git-cz",
    "release": "standard-version",
    "electron:dev": "nodemon -L -w ember-electron -x 'yarn run-s clean:tmp electron:start'",
    "electron:start": "ember electron",
    "clean": "run-p clean:*",
    "clean:tmp": "rimraf tmp",
    "clean:dist": "rimraf dist",
    "clean:electron-out": "rimraf electron-out"
  },
  "nodemonConfig": {
    "events": {
      "start": "yarn notify -t 'ember-electron' -m 'Starting Electron app...' -i ember-electron/icons/notification.png",
      "restart": "yarn notify -t 'ember-electron' -m 'Restarting Electron app...' -i ember-electron/icons/notification.png",
      "crash": "yarn notify -t 'ember-electron' -m 'Electron app has crashed!' -s Funk -i ember-electron/icons/notification.png"
    }
  },
  "build": {
    "appId": "org.nano.wallet"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "forge": "./ember-electron/electron-forge-config.js",
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "dependencies": {
    "connect": "^3.6.6",
