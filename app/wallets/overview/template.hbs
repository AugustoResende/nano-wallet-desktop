{{!-- <h1>{{t 'overview'}}</h1> --}}

{{!-- {{wallet-overview wallet=model accounts=model.accounts createAccount=(route-action 'createAccount') page=page perPage=perPage}} --}}
{{navigation-bar wallet=model createAccount=(route-action 'createAccount')}}

<main role="main">

    {{total-amount wallet=model}}

    <section>
        <div class="container-fluid">
            <div class="row">
                <hr>
            </div>
        </div>
    </section>

    <section>
        <div class="container-fluid">
            <div class="row">
                <div class="col">
                    {{#slick-slider autoplay=false slidesToShow=(if (lte model.accounts.length 3) 1 3) afterChange='afterChange' centerPadding='60px' centerMode=true slidesToScroll=1
                    arrows=true class="main-gallery"}} {{#each model.accounts as |account|}} {{gallery-slide model=account}}
                    {{/each}} {{/slick-slider}}
                </div>
            </div>
        </div>
    </section>

    <section>
        <div class="container">
            <div class="row">
                <div class="col">
                    {{#link-to 'wallets.send' model class="arrow"}} {{fa-icon "chevron-up"}} {{/link-to}}
                </div>
            </div>
        </div>
    </section>

    <section>
        <div class="container">
            <div class="row">
                <div class="col">
                    
                </div>
            </div>
        </div>
    </section>

    {{send-button model=model}} {{#each flashMessages.arrangedQueue as |flash|}} {{flash-message flash=flash}} {{/each}}

    <div class="row">
        <div class="col">
            {{outlet}}
        </div>
    </div>
</main>

